      <ComboBox fx:id="cbx_camp" layoutX="58.0" layoutY="51.0" prefWidth="150.0" />
      <TextField fx:id="txt_cedula" layoutX="327.0" layoutY="51.0" promptText="cedula" />
      <DatePicker fx:id="dtp_registro" layoutX="58.0" layoutY="179.0" promptText="registro" />
      <CheckBox fx:id="chx_tatuaje" layoutX="336.0" layoutY="183.0" mnemonicParsing="false" onAction="#acc_tautaje" text="No tiene tatuaje" />
